{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@prisma/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@prisma/prisma": "<rootDir>/../libs/prisma/src",
    "@exceptions/exceptions/(.*)": "<rootDir>/../libs/exceptions/src/$1",
    "@exceptions/exceptions": "<rootDir>/../libs/exceptions/src",
    "@types/types/(.*)": "<rootDir>/../libs/types/src/$1",
    "@types/types": "<rootDir>/../libs/types/src",
    "@entyties/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@entyties/entities": "<rootDir>/../libs/entities/src",
    "@logger/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@logger/logger": "<rootDir>/../libs/logger/src"
  }
}